extends layout

block content
    .w3-card-4
        header.w3-container.w3-teal
            h3.w3-center Classe : #{classe}    
        .w3-container
                header.w3-container.w3-teal.w3-margin-top
                    h4.w3-center Indiv√≠duos
                table(class="w3-table-all")
                    each i in individuos
                        tr
                            td
                                a(href= prefixo + repositorio + "/classe/" + classe + "/individuo/" + i)= i
        footer(class="w3-container w3-teal")
            adress.w3-right Diogo Pereira
            a(href="javascript:history.back()") Voltar
            adress /
            a(href="/") Home